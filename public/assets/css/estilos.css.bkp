    cursor: pointer;
    height: 24px;
    min-width: 50px;
    text-align: center;
    margin-right: 40px;
}

.nav2 > .container > div:hover{
    border-bottom: 2px solid #CCCCCC;
}

.nav2 > .container > div.active{
    color: #FF149B;
    border-bottom: 2px solid #FF149B;
}

.nav2 > .container > div > a{
    width: 100%;
    height: 100%;
    color: inherit;
}

.nav2 > .container > div > a:hover{
    text-decoration: none;
}

