.sidebar{
    box-shadow: 1px 1px 5px #cccccc;
    max-height: 360px; 
    min-height: 25%;
    background: #fff; 
    border: none; 
    border-radius: 2px;
    text-align: left;
    
}

.sidebar > .elementos{
    overflow-y: auto;
    display: block;
    height: 325px;
}

.sidebar > .titulo{
    height: 35px;
}

.sidebar > .elementos > ul{
    list-style-type: none;
    padding-left: 0px;
}

.sidebar > .elementos > ul > li{
    padding-left: 14px;
    padding-bottom: 3px;
    padding-top: 3px;  
    display: flex;
    justify-content: center;
    align-content: center;
    flex-direction: column;
    cursor: pointer;
}

.sidebar > .elementos > ul > li:hover{
    background: #cccccc;
    color: #000;
    box-shadow: 1px 1px 5px #cccccc;

}

.sidebar > .elementos > ul > li > a{
    color: #333333;
}

.sidebar > .elementos > ul > li.active{
    /*background: #FF149B;*/
    color: #FF149B;
    /*box-shadow: 1px 1px 5px #cccccc; */ 
    cursor: default !important;
}

.sidebar > .elementos > ul > li.active:hover{
    background: #fff;
    color: #FF149B;
    box-shadow: none; 
    cursor: default !important;
}

.sidebar > .elementos > ul > li.active > a{
    color: #FF149B;
}

.sidebar > .elementos > ul > li.active > a > div{
    cursor: default !important;
}

.sidebar > .elementos > ul > li.active > a > div > label{
    cursor: default !important;
}

.sidebar > .elementos > ul > li.active > a > div > i.check{
    cursor: default !important;
}

.sidebar > .elementos > ul > li.active > a > div > i.cross{
    cursor: default !important;
}

.sidebar > .elementos > ul > li > a > div > i.check{
    color: #58B45A; 
    margin-right: 5px;
    cursor: pointer;
}

.sidebar > .elementos > ul > li > a > div > i.cross{
    color: #C6383D; 
    margin-right: 5px;
    cursor: pointer;
}

.sidebar > .titulo{
    text-align: center;
    font-size: 22px;
    color: #333333;
    font-weight: bold;
    cursor:pointer;
}

.nav2{
    background: #fff;
    z-index: 1010;
    height: 35px;
    width: 100%;
