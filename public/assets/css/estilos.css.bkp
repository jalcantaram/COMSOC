    color: #333333;
    margin-bottom: 28px;
}

h2{
    color: #333333;
    font-size: 28px !important;
    margin-top: 5px;
    margin-bottom: 28px;
}

th{
    text-align: center !important;
}

.botonesNotify{
    border: 1px solid #C6383D;
    border-radius: 2px;
    font-weight: normal;
    color: #C6383D; 
    background-color:white;
    margin-left: 10px;
    margin-right: 10px;
    width: 70px;
    text-shadow: -1px 1px #eeeeee;
}

.botonesNotify:hover{
    text-shadow: -1px 1px #cccccc;
